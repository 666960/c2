form zOrder
{
    theme = res/common/default.theme
    style = basic
    size = 600, 600
    scroll = SCROLL_BOTH

    label label1
    {
        style = basic
        autoWidth = true
        height = 200
        text = TOP
        zIndex = 3
    }

    label label2 : label1
    {
        text = MID
        position = 0, 100
        zIndex = 2
    }

    label label3 : label1
    {
        text = BOTTOM
        position = 0, 200
        zIndex = 1
    }
}